                    android:text="学历"
                    android:textColor="@color/font_black" />

                <TextView
                    android:id="@+id/tv_personal_info_xueli"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/x10"
                    android:layout_marginRight="@dimen/x10"
                    android:drawablePadding="@dimen/x10"
                    android:drawableRight="@drawable/go_next"
                    android:gravity="center_vertical|right"
                    android:textColor="@color/font_black" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_gray_divider" />

            <LinearLayout
                android:id="@+id/rl_personal_qq"
                android:layout_width="match_parent"
                android:layout_height="@dimen/y35"
                android:background="@color/white" >

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="@dimen/x5"
                    android:layout_marginRight="@dimen/x5" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical"
                    android:gravity="center_vertical"
                    android:text="QQ"
                    android:textColor="@color/font_black" />

                <TextView
                    android:id="@+id/tv_personal_info_qq"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/x10"
                    android:layout_marginRight="@dimen/x10"
                    android:drawablePadding="@dimen/x10"
                    android:drawableRight="@drawable/go_next"
                    android:gravity="center_vertical|right"
                    android:textColor="@color/font_black" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_gray_divider" />

            <LinearLayout
                android:id="@+id/rl_personal_matrimony_type"
                android:layout_width="match_parent"
                android:layout_height="@dimen/y35"
                android:background="@color/white" >

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="@dimen/x5"
                    android:layout_marginRight="@dimen/x5" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical"
                    android:gravity="center_vertical"
                    android:text="婚姻状况"
                    android:textColor="@color/font_black" />

                <TextView
                    android:id="@+id/tv_personal_info_hunyin"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/x10"
                    android:layout_marginRight="@dimen/x10"
                    android:drawablePadding="@dimen/x10"
                    android:drawableRight="@drawable/go_next"
                    android:gravity="center_vertical|right"
                    android:textColor="@color/font_black" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_gray_divider" />

            <LinearLayout
                android:id="@+id/rl_personal_industry"
                android:layout_width="match_parent"
                android:layout_height="@dimen/y35"
                android:background="@color/white" >

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="@dimen/x5"
                    android:layout_marginRight="@dimen/x5" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical"
                    android:gravity="center_vertical"
                    android:text="所属行业"
                    android:textColor="@color/font_black" />

                <TextView
                    android:id="@+id/tv_personal_info_hangye"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/x10"
                    android:layout_marginRight="@dimen/x10"
                    android:drawablePadding="@dimen/x10"
                    android:drawableRight="@drawable/go_next"
                    android:gravity="center_vertical|right"
                    android:text=""
                    android:textColor="@color/font_black" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_gray_divider" />

            <LinearLayout
                android:id="@+id/rl_personal_monthly_income"
                android:layout_width="match_parent"
                android:layout_height="@dimen/y35"
                android:background="@color/white" >

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="@dimen/x5"
                    android:layout_marginRight="@dimen/x5" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical"
                    android:gravity="center_vertical"
                    android:text="月均收入"
                    android:textColor="@color/font_black" />

                <TextView
