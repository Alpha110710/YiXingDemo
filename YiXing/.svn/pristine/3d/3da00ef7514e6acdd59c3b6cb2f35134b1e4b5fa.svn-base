                    android:layout_width="@dimen/x6"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/x10"
                    android:src="@drawable/go_next" />

                <com.yixing.ui.widget.CircleImageView
                    android:id="@+id/iv_personal_info_head_icon"
                    android:layout_width="@dimen/x40"
                    android:layout_height="@dimen/x40"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/x10"
                    android:layout_toLeftOf="@id/iv_personal_info_head_go_next"
                    android:src="@drawable/my_account5" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_gray_divider"
                android:visibility="gone" />

            <LinearLayout
